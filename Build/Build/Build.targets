		<Exec Command="&quot;$(MSBuildToolsPath)\MSBuild.exe&quot; &quot;$(ProjectDir)RunTasks.targets&quot; /nologo /verbosity:minimal /property:Configuration=$(Configuration);OutDir=$(OutDir)" />
	</Target>
</Project>